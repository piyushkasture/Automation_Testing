{
  "request": {
    "method": "POST",
    "url": "/api/users",
    "headers": {
      "Content-Type": {
        "equalTo": "application/json"
      }
    },
    "bodyPatterns": [
      {
        "matchesJsonPath": "$.name"
      }
    ]
  },
  "response": {
    "status": 201,
	"fixedDelayMilliseconds": 3000,
    "headers": {
      "Content-Type": "application/json"
    },
    "bodyFileName": "user-created.json"
  }
}
